{
  "name": "@daisy-chain/components",
  "version": "1.0.33",
  "description": "Daisy Chain Stencil Components",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/components/components.esm.js",
  "files": [
    "dist/",
    "loader/",
    "hydrate/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "yarn generate-icons",
    "generate-icons": "node ./scripts/generate-icons.js",
    "prepublish": "yarn build",
    "format": "prettier --write \"package.json\" \"src/**/*.{html,tsx,ts,css,json}\" \"!src/**/*.d.ts\""
  },
  "dependencies": {
    "@stencil/core": "^2.0.1",
    "@stencil/react-output-target": "^0.0.9",
    "web-animations-js": "^2.3.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "26.0.21",
    "@types/puppeteer": "5.4.3",
    "fast-glob": "^3.2.5",
    "handlebars": "^4.7.7",
    "hast-util-to-html": "^7.1.2",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "prettier": "^2.2.1",
    "puppeteer": "5.5.0",
    "svg-parser": "^2.0.4",
    "svgo": "1.3.2"
  },
  "gitHead": "141a554a4b68758b4f4cc7809bf4149a7f439d56",
  "publishConfig": {
    "access": "public"
  }
}
